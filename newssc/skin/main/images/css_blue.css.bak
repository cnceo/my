.popupModal table tr:hover{border-bottom:#aaa solid 1px;}
.popupModal table tr td.title{width:25%; text-align:center; border-right:#fff solid 1px;background:#cfcfcf;}
.popupBox{width:400px; height:200px; margin:0 auto;autoborder:none; overflow-x:none;overflow-y:none; font-size:16px;}
.dantuo{text-align: center; margin-top:-5px;}
/* 聊天室 */
.right-body{background-position:-430px -271px; width:193px; height:156px; _height:146px;overflow:visible;min-height:146px; padding:0 7px;}
.right-body-tallWid{height:338px;}
.right-body .cont-black{background:#444; width:193px;border-top:2px solid #333;border-left:1px solid #333;height:148px;word-break: break-all; word-wrap:break-word;}
.right-body .cont-black p{color:#fff;text-indent:2em;}
.right-body .cont-black p a{color:#fff;}
.right-body .talk-t{width:182px;height:214px; border-top:2px solid #0B3E45; border-bottom:1px solid #ccc;border-right:1px solid #ccc;border-left:1px solid #0B3E45; color:#fff; padding:0 5px; overflow-y:auto}
.right-body .cont-blue{background:#0E555E;}
.right-body .talk-input{border-top:2px solid #999;border-left:1px solid #999; border-bottom:1px solid #ccc; border-right:1px solid #ccc; width:190px; height:75px; margin-top:3px}
.right-body .bottom-btn{ height:30px;}
.right-body .face-btn{background-position:-222px -446px; width:97px; height:30px;float:left; text-align:center; line-height:30px; font-size:12px; font-family:"Microsoft YaHei","微软雅黑","Microsoft JhengHei","宋体"; font-weight:bold; color:#333; position:relative}
.right-body .send-btn{background-position:-347px -450px; width:62px; height:23px; margin-left:20px; margin-top:4px;float:left;text-align:center; line-height:23px; font-size:12px; font-family:"Microsoft YaHei","微软雅黑","Microsoft JhengHei","宋体"; font-weight:bold; color:#333; cursor:pointer;}
.right-body .face-btn:hover {background-position:-222px -477px; color:#000}
.right-body .send-btn:hover {background-position:-347px -481px;color:#000}
.right-body .face-btn .faceBox{width:130px; height:auto; position:absolute; left:0; top:-128px; border:#ccc 1px solid; background:#fff; display:none;cursor:pointer; }
.right-body .face-btn .faceBox img{width:32px;height:32px; border:none; margin:0; float:left;}
.right-bottom{background-position:-430px -608px; width:207px; height:5px;}
.talkBoxCss1{color:#999;}
.talkBoxCss2{color:#FF0;}
.talkBoxCss3{color:#fff;display:block;}
/* 聊天室结束 */
.userxz{margin:0; padding:0; width:450px}
.userxz td{border:#3a3a3a 2px solid;text-align:center;}
.userxz .userxz-t{ background:#eee;}
.voice-on{background:url(images/voice-on.png) no-repeat;outline:none;width:21px; border:none; height:16px; position:absolute; right:48px; top:10px; cursor:pointer;}
.voice-off{background:url(images/voice-off.png) no-repeat;outline:none;width:21px; border:none; height:16px; position:absolute; right:48px; top:10px; cursor:pointer;}
/* 公共展示结束 */
.notice{width:700px; margin:0 auto;}
.notice h3{font-size:16px; color:#000; line-height:25px; text-align:center;}
.notice h3 .title{text-align:center; width:100%;}
.notice .noticedetailed{line-height:24px;}
.notice .noticdate{width:100%; text-align:center; color:#666; line-height:20px;}
